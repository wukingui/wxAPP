/* pages/index/index.wxss */
.container {
  display: block;
  height: 100%;
  width: 100%;
}
.userinfo-avatar {
  display: block;
  height: 280rpx;
  width: 100%;
}
.nav {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 80rpx;
  width: 100%;
  color: #fff;
  background: url(http://aimg8.dlszyht.net.cn/ev_user_module_content_tmp/2018_05_03/tmp1525307555_1653180_s.png);
}
.nav li {
  width: 25%;
  height: 100%;
}
.nav li navigator {
  display: block;
  width: 100%;
  height: 100%;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search {
  height: 100.8rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  
}
.search input {
  width: 530rpx;
  height: 60rpx;
  font-size: 28rpx;
  padding: 8epx;
  background-color: #f5f5f5;
}
.search button {
  width: 178rpx;
  height: 60rpx;
  margin: 0;
  padding: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #fff;
  background-color: #da251d;
}
.customModule {
  width: 100%;
  height: 308.8rpx;
  background: #ccc;
}
.customModule image {
  width: 100%;
  height: 100%;
}
.product .title {
  width: 100%;
  height: 105.6rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.title .title-left {
  width: 210rpx;
  height: 2rpx;
  background: #D02E2E;
}
.title .content {
  width: 270.4rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.title .content image {
  width: 20rpx;
  height: 32rpx;
}
.title .content .product {
  width: 200rpx;
  height: 44rpx;
  color: #fff;
  font-size: 28rpx;
  background: #D02E2E;
  padding: 4rpx 16rpx;
  border-radius: 6rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.title .title-right {
  width: 210rpx;
  height: 2rpx;
  background: #D02E2E;
}